{
  "version": 2,
  "projects": {
    "rocketchat-desktop-electron": {
      "root": "apps/rocketchat-desktop-electron",
      "sourceRoot": "apps/rocketchat-desktop-electron/src",
      "projectType": "application",
      "prefix": "rocketchat-desktop-electron",
      "targets": {
        "build": {
          "executor": "nx-electron:build",
          "options": {
            "outputPath": "dist/apps/rocketchat-desktop-electron",
            "main": "apps/rocketchat-desktop-electron/src/main.ts",
            "tsConfig": "apps/rocketchat-desktop-electron/tsconfig.app.json",
            "assets": [
              "apps/rocketchat-desktop-electron/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/rocketchat-desktop-electron/src/environments/environment.ts",
                  "with": "apps/rocketchat-desktop-electron/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "nx-electron:execute",
          "options": {
            "buildTarget": "rocketchat-desktop-electron:build"
          }
        },
        "package": {
          "executor": "nx-electron:package",
          "options": {
            "name": "rocketchat-desktop-electron",
            "frontendProject": "rocketchat-desktop-frontend",
            "outputPath": "dist/packages",
            "prepackageOnly": true
          }
        },
        "make": {
          "executor": "nx-electron:make",
          "options": {
            "name": "rocketchat-desktop-electron",
            "frontendProject": "rocketchat-desktop-frontend",
            "outputPath": "dist/executables"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/rocketchat-desktop-electron/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/rocketchat-desktop-electron"
          ],
          "options": {
            "jestConfig": "apps/rocketchat-desktop-electron/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "nx-electron"
  },
  "defaultProject": "rocketchat-desktop-electron"
}
