{
  "version": 2,
  "projects": {
    "rocketchat-desktop-electron": {
      "root": "apps/rocketchat-desktop-electron",
      "sourceRoot": "apps/rocketchat-desktop-electron/src",
      "projectType": "application",
      "prefix": "rocketchat-desktop-electron",
      "targets": {
        "build": {
          "executor": "nx-electron:build",
          "options": {
            "outputPath": "dist/apps/rocketchat-desktop-electron",
            "main": "apps/rocketchat-desktop-electron/src/main.ts",
            "tsConfig": "apps/rocketchat-desktop-electron/tsconfig.app.json",
            "assets": ["apps/rocketchat-desktop-electron/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/rocketchat-desktop-electron/src/environments/environment.ts",
                  "with": "apps/rocketchat-desktop-electron/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "nx-electron:execute",
          "options": {
            "buildTarget": "rocketchat-desktop-electron:build"
          }
        },
        "package": {
          "executor": "nx-electron:package",
          "options": {
            "name": "rocketchat-desktop-electron",
            "frontendProject": "rocketchat-desktop-frontend",
            "outputPath": "dist/packages",
            "prepackageOnly": true
          }
        },
        "make": {
          "executor": "nx-electron:make",
          "options": {
            "name": "rocketchat-desktop-electron",
            "frontendProject": "rocketchat-desktop-frontend",
            "outputPath": "dist/executables"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/rocketchat-desktop-electron/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/rocketchat-desktop-electron"],
          "options": {
            "jestConfig": "apps/rocketchat-desktop-electron/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "rocketchat-desktop-frontend": {
      "root": "packages/rocketchat-desktop-frontend",
      "sourceRoot": "packages/rocketchat-desktop-frontend/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/rocketchat-desktop-frontend",
            "index": "packages/rocketchat-desktop-frontend/src/index.html",
            "main": "packages/rocketchat-desktop-frontend/src/main.tsx",
            "polyfills": "packages/rocketchat-desktop-frontend/src/polyfills.ts",
            "tsConfig": "packages/rocketchat-desktop-frontend/tsconfig.app.json",
            "assets": [
              "packages/rocketchat-desktop-frontend/src/favicon.ico",
              "packages/rocketchat-desktop-frontend/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/rocketchat-desktop-frontend/src/environments/environment.ts",
                  "with": "packages/rocketchat-desktop-frontend/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "rocketchat-desktop-frontend:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "rocketchat-desktop-frontend:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/rocketchat-desktop-frontend/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/rocketchat-desktop-frontend"],
          "options": {
            "jestConfig": "packages/rocketchat-desktop-frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "rocketchat-desktop-frontend-e2e": {
      "root": "packages/rocketchat-desktop-frontend-e2e",
      "sourceRoot": "packages/rocketchat-desktop-frontend-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "packages/rocketchat-desktop-frontend-e2e/cypress.json",
            "devServerTarget": "rocketchat-desktop-frontend:serve",
            "tsConfig": "packages/rocketchat-desktop-frontend-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "rocketchat-desktop-frontend:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/rocketchat-desktop-frontend-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "nx-electron"
  },
  "defaultProject": "rocketchat-desktop-electron",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  }
}
